# Database (Vercel Postgres — auto-populated when you add the integration)
POSTGRES_URL=

# Auth (NextAuth.js v5)
# Generate with: npx auth secret
AUTH_SECRET=
# Set to your production URL on Vercel (e.g., https://firevector.org)
AUTH_URL=http://localhost:3000

# Google OAuth (console.cloud.google.com → APIs & Services → Credentials)
# Authorized redirect URI: https://firevector.org/api/auth/callback/google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Mapbox (account.mapbox.com → Tokens)
NEXT_PUBLIC_MAPBOX_TOKEN=
